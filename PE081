#include <iostream>
using namespace std;
#include <fstream>

int main(){
	int mat[80][80];
	ifstream myfile, myfile1;
	myfile.open("p081_matrix.txt");
    for (int j = 0; j < 80; j++){
		string text;
		myfile>>text;
    	ofstream temp;
    	temp.open("p081_matrix1.txt");
    	for (int i = 0; i < text.length(); i++){
	    	if (text[i] == ',')
	    		temp<<' ';
	    	else temp<<text[i];
		}
		temp.close();
		int read;
		myfile1.open("p081_matrix1.txt");
		for (int i = 0; i < 80; i++){
			myfile1>>mat[j][i];
		}
		myfile1.close();
	}
	int sum_matrix[80][80];
	int sum[80];
	for (int i = 0; i < 80; i++){
		sum_matrix[i][79] = mat[i][79];
		for (int j = i + 1; j < 80; j++){
			sum_matrix[i][79] = sum_matrix[i][79] + mat[j][79];
		}
	}
	for (int i = 78; i >= 0 ; i--){
		for (int k = 0; k < 80; k++){
			sum[k] = mat[k][i] + sum_matrix[k][i + 1];
			int temp_sum = 0;
				for (int j = k; j < 80; j++){
					temp_sum = temp_sum + mat[j][i];
					if (temp_sum + sum_matrix[j][i + 1] < sum[k])
						sum[k] = temp_sum + sum_matrix[j][i + 1];
						sum_matrix[k][i] = sum[k];
				}
			}
			cout<<endl;
		}
	int min = 100000000;
	for (int i = 0; i < 80; i++){
		for ( int j = 0; j < i; j++){
			sum_matrix[i][0] = sum_matrix[i][0] + mat[j][0];
		}
	}
	for (int i = 0; i < 80; i++){
		if (sum_matrix[i][0] < min)
			min = sum_matrix[i][0];
	}
	cout<<min;
	return 0;
}
