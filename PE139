#include <iostream>
using namespace std;
#include <math.h>
bool is_coprime(int a, int b){
	int temp;
	for (int i = 2; i <= floor(sqrt(a)); i++){
		//cout<<i<<endl;
		if (a % i == 0){
			temp = a/i;
			//cout<<i<<" "<<temp<<" "<<b % i<<endl;
			if (b % i == 0)
				return false;
			
			if (b % temp == 0)
				return false;
			
		}
	}
	return true;
}

int main(){
	int count = 0;
	unsigned long long a, b, hyp;
	for (int m = 2; m <= floor(sqrt(100000000)); m++){
		if (m % 1000 == 0)
			cout<<m<<endl;
		int initial;
			if (m % 2 == 0)
				initial = 1;
			else 
				initial = 2;
			int n = initial;
			while (n < m){
				if (is_coprime(m,n)){
					a = m*m - n*n;
					b = 2*m*n;
					hyp = m*m + n*n;
					if (hyp % (a - b) == 0){
						count++;
						cout<<a<<" "<<b<<" "<<hyp<<endl;
						int k = 2;
						while (k*(a + b + hyp) <= 100000000){
							count++;
							k++;
						}
					}
			}
		n = n + 2;	
		}
	}
	cout<<count;
	return 0;
}
